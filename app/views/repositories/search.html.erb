<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>
<div>
  <ul>
    <% @results.each do |result| %>
      <li>
        <%= result["name"] %><br>
        <%= result["html_url"] %><br>
        <%= result["description"] %><br>
      </li>
    <% end %>
  </ul>
</div>
